import pandas as pd

logs = pd.read_csv('/datasets/logs.csv')
logs['email'] = logs['email'].fillna(value='')
# мой код ниже
logs.loc[logs['source'] == ''] = 'email'
#
print(logs['source'].value_counts()) #одинаково "неверные" значения переменной logs
